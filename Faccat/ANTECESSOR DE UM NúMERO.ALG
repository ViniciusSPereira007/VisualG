// Algoritmo "Antecessor de um número"
var
   numero, antecessor: inteiro
inicio
   // Leia o valor
   escreval("Digite um valor inteiro: ")
   leia(numero)

   // Calcule o antecessor
   antecessor := numero - 1

   // Exiba o antecessor
   escreval("O antecessor de ", numero, " é ", antecessor)
fimalgoritmo
